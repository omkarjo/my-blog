#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules/@astrojs/language-server/bin/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules/@astrojs/language-server/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules/@astrojs/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules/@astrojs/language-server/bin/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules/@astrojs/language-server/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules/@astrojs/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+language-server@2._218dfae25b025236f32ee6f380724084/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@astrojs/language-server/bin/nodeServer.js" "$@"
else
  exec node  "$basedir/../@astrojs/language-server/bin/nodeServer.js" "$@"
fi
