#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules/@astrojs/check/dist/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules/@astrojs/check/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules/@astrojs/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules/@astrojs/check/dist/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules/@astrojs/check/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules/@astrojs/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/@astrojs+check@0.9.4_pretti_9036a911fc4313162dffec01ed4e8583/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@astrojs/check/dist/bin.js" "$@"
else
  exec node  "$basedir/../@astrojs/check/dist/bin.js" "$@"
fi
