#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/astro@5.13.9_@types+node@22_7df8161c7b7dbe119433faf482890c21/node_modules/astro/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/astro@5.13.9_@types+node@22_7df8161c7b7dbe119433faf482890c21/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/astro@5.13.9_@types+node@22_7df8161c7b7dbe119433faf482890c21/node_modules/astro/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/astro@5.13.9_@types+node@22_7df8161c7b7dbe119433faf482890c21/node_modules:/proc/cygdrive/c/Users/omkar/Documents/Blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
